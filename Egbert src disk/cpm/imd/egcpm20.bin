      STORE count + 1 TO count
   ENDDO
   RELEASE count
   ERASE
   RETURN
ENDIF
SET INDEX TO
GO TOP
DO WHILE T
   STORE " " TO pause
   STORE T TO badans
   STORE " " TO mchoice
   IF *
     @ 1,50 SAY "DELETED"
   ELSE
     @ 1,50 SAY "       "
