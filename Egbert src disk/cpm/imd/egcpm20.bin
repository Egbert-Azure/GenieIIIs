	IF .NOT. EOF
	   LOOP
	ELSE
	   @ 21,27 SAY "This is the last record"
	   GO BOTTOM
	   STORE 1 TO count
	   DO WHILE count < 50
	      STORE count + 1 TO count
	   ENDDO
	   RELEASE count
	   @ 21,0
	   LOOP
	ENDIF
      CASE mchoice = "F"
