              READ
              @ line,48
              @ line+1,48
              IF an = "Y"
                * erase current deposit from screen...
                @ line,3 SAY "                           "
                @ line,31 SAY "            "
                DELETE
                SKIP
                IF EOF
                  STORE F TO more
                ENDIF
                LOOP
              ELSE
                IF special
                  REPLACE Paidfrom WITH mpayfrom,Amt WITH mamt
                ENDIF
              ENDIF
            ELSE
