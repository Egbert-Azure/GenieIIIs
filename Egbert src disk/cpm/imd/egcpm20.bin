   @ 19,45 SAY "|"
   @ 18,45 SAY "|"
   @ 17,45 SAY "|"
   @ 16,45 SAY "|"
   @ 15,45 SAY "|"
   @ 14,45 SAY "|"
   @ 13,45 SAY "|"
   @ 12,45 SAY "|"
   @ 11,45 SAY "|"
   @ 10,45 SAY "|"
   @  9,45 SAY "|"
   @  8,45 SAY "|"
   @  7,45 SAY "|"
   @  6,45 SAY "|"
   @  5,45 SAY "|"
   @  4,45 SAY "|"
   @  6, 3 SAY "========================================"
   @  7,30 SAY "|"
   @  8,30 SAY "|"
   @  9,30 SAY "|"
   @ 10,30 SAY "|"
   @ 11,30 SAY "|"
   @ 12,30 SAY "|"
   @ 13,30 SAY "|"
   @ 14,30 SAY "|"
   @  2,18 SAY "DEPOSIT SLIP"
