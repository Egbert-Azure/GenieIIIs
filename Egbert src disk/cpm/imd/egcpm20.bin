* Program..: FI-LOCAT.CMD
* Author...: Your Name
* Date.....: 00/00/00
* Notice...: Copyright 1900, All Rights Reserved
*
STORE " " TO expression,string
* ---LOCATE section.
DO WHILE expression = " "
   @ 10,0 SAY 'EXAMPLE :STATE="CA"'
   @ 12,0 SAY clearline
   *
   @ 11,0 SAY "-"
   ACCEPT "Enter LOCATE expression " TO expression
   @ 10,0 SAY clearline
   STORE TRIM(expression) TO expression
   DO CASE
      CASE expression = " "
      * ---Exit.
         RETURN
      CASE 0 = TEST(&expression)
      * ---INVALID EXPRESSION.
         @ 13,0 SAY clearline
         STORE " " TO select
         @ 13,0 SAY "INVALID EXPRESSION: "+;
                    "Strike any key to continue... ";
                GET select
         READ NOUPDATE
         @ 13,0 SAY clearline
         STORE " " TO expression
      OTHERWISE
      * ---LOCATE the record.
         * ---Close index file for a faster LOCATE.
         SET INDEX TO
         LOCATE FOR &expression
         IF .NOT. EOF
            * ---Found a matching record.
            STORE # TO lastrecord
            * ---Reopen index file.
            SET INDEX TO DATE
            GOTO lastrecord
         ELSE
            * ---Reopen index file and
            * ---reset EOF marker to true (.T.).
            SET INDEX TO DATE
            GO BOTTOM
            SKIP
         ENDIF
   ENDCASE
ENDDO
IF poschoice = "L"
   * ---Return to calling program if only the LOCATE was desired.
   RETURN
ENDIF
*
* ---DISPLAY section.
STORE " " TO string
DO WHILE string = " "
   @ 10,0 SAY "EXAMPLE :NAME+ADDRESS"
   @ 12,0 SAY "E"
   ACCEPT "Enter DISPLAY string " TO string
   @ 10,0 SAY clearline
   STORE TRIM(string) TO string
   DO CASE
      CASE string = " "
      * ---Exit.
         @ 12,0 SAY clearline
         @ 13,0 SAY clearline
         RETURN
      CASE 0 = TEST(&string)
      * ---INVALID EXPRESSION.
         @ 13,0 SAY clearline
         STORE " " TO select
         @ 13,0 SAY "INVALID DISPLAY EXPRESSION: "+;
                    "Strike any key to continue... ";
                GET select
         READ NOUPDATE
         @ 13,0 SAY clearline
         STORE " " TO string
   ENDCASE
ENDDO
* ---Now, DISPLAY the expression.
STORE F TO is:eof,is:some
DO WHILE .NOT. is:eof
   * ---The following set of dBASE II commands are to 
   * ---clear-to-end-of-screen.  If you have an IBM-PC,
   * ---you can replace these commands with the single
   * ---command,  @ 4,0 ERASE
   *
   @  4,0 SAY clearline
